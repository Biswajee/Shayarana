<div class="row">
  <div class="col-md-12 col-sm-12">
    

<div class="row">
  <div class="col-md-8 col-sm-6">
<h1>Add Posts</h1>
<%= form_for :post, url: posts_path do |f| %>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :body %><br>
  <%= f.text_area :body %>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>
</body>
</html>
